{
  "mcpServers": {
    "serena": {
      "command": "/opt/homebrew/bin/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--enable-web-dashboard",
        "false"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "asana": {
      "command": "npx",
      "args": ["mcp-remote", "https://mcp.asana.com/sse"]
    },
    "genie-feaz": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-984752964297111.11.azuredatabricks.net/api/2.0/mcp/genie/01f07200699f1c41a45931716cce08e0",
        "--header",
        "Authorization: Bearer ${DAPI_FEAZ}"
      ]
    },
    "uc-functions-feaz": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-984752964297111.11.azuredatabricks.net/api/2.0/mcp/functions/robkisk/c360",
        "--header",
        "Authorization: Bearer ${DAPI_FEAZ}"
      ]
    },
    "vs-feaz": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-984752964297111.11.azuredatabricks.net/api/2.0/mcp/vector-search/robkisk/c360",
        "--header",
        "Authorization: Bearer ${DAPI_FEAZ}"
      ]
    },
    "dbsql-feaz": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-984752964297111.11.azuredatabricks.net/api/2.0/mcp/sql",
        "--header",
        "Authorization: Bearer ${DAPI_FEAZ}"
      ]
    },
    "genie-dev": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-2951351452776088.8.azuredatabricks.net/api/2.0/mcp/genie/01f0b4b2aa5b1b64a7f5e827e8a11137",
        "--header",
        "Authorization: Bearer ${DAPI_DEV}"
      ]
    },
    "uc-functions-dev": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-2951351452776088.8.azuredatabricks.net/api/2.0/mcp/functions/bu1_dev/retail_c360",
        "--header",
        "Authorization: Bearer ${DAPI_DEV}"
      ]
    },
    "vs-dev": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-2951351452776088.8.azuredatabricks.net/api/2.0/mcp/vector-search/bu1_dev/ai_agent",
        "--header",
        "Authorization: Bearer ${DAPI_DEV}"
      ]
    },
    "dbsql-dev": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://adb-2951351452776088.8.azuredatabricks.net/api/2.0/mcp/sql",
        "--header",
        "Authorization: Bearer ${DAPI_DEV}"
      ]
    },
    "dbx-local-dev": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/robby.kiskanyan/dev/mcp/projects/demos/databricks-mcp",
        "run",
        "databricks-mcp-server"
      ],
      "env": {
        "DATABRICKS_HOST": "https://adb-2951351452776088.8.azuredatabricks.net",
        "DATABRICKS_TOKEN": "${DAPI_DEV}",
        "DATABRICKS_WAREHOUSE_ID": "${DB_WH_ID_DEV}",
        "RUNNING_VIA_CURSOR_MCP": "true"
      }
    },
    "dbx-app-prod": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--refresh",
        "--from",
        "git+https://github.com/robkisk/custom-mcp-databricks-app.git",
        "dba-mcp-proxy",
        "--databricks-host",
        "https://adb-985990099698746.6.azuredatabricks.net",
        "--databricks-app-url",
        "https://dbx-custom-mcp-app-985990099698746.6.azure.databricksapps.com"
      ],
      "env": {}
    },
    "terraform": {
      "type": "stdio",
      "command": "docker",
      "args": ["run", "-i", "--rm", "hashicorp/terraform-mcp-server"],
      "env": {}
    },
    "databricks-sdk-py Docs": {
      "command": "npx",
      "args": ["mcp-remote", "https://gitmcp.io/databricks/databricks-sdk-py"]
    },
    "databricks-blog-posts Docs": {
      "command": "npx",
      "args": [
