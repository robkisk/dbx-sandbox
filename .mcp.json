      "args": [
        "mcp-remote",
        "https://gitmcp.io/anthropics/skills"
      ]
    },
    "Hobson Agents": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/wshobson/agents"
      ]
    }
